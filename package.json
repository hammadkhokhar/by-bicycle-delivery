{
  "name": "by-bicycle-delivery",
  "version": "0.0.1",
  "license": "MIT",
  "description": "An advanced microservice transforming last-mile logistics. It offers seamless ordering features, including quotation and booking.",
  "prisma": {
    "schema": "src/app/v1/prisma/schema.prisma"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node --unhandled-rejections=strict ./dist/src/app.js",
    "debug": "DEBUG=* npm run start",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "test": "jest --detectOpenHandles",
    "ready": "yarn install && yarn prisma generate && yarn start"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "bullmq": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "redis": "^4.6.12",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@prisma/client": "5.7.1",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
